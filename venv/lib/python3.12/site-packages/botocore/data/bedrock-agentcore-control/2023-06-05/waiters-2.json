        "state" : "success",
        "expected" : "ACTIVE"
      }, {
        "matcher" : "path",
        "argument" : "status",
        "state" : "failure",
        "expected" : "CREATE_FAILED"
      }, {
        "matcher" : "path",
        "argument" : "status",
        "state" : "failure",
        "expected" : "UPDATE_FAILED"
      }, {
        "matcher" : "path",
        "argument" : "status",
        "state" : "failure",
        "expected" : "DELETE_FAILED"
      } ]
    },
    "PolicyEngineDeleted" : {
      "description" : "Wait until a PolicyEngine is deleted",
      "delay" : 2,
      "maxAttempts" : 60,
      "operation" : "GetPolicyEngine",
      "acceptors" : [ {
        "matcher" : "error",
        "state" : "success",
        "expected" : "ResourceNotFoundException"
      }, {
        "matcher" : "path",
        "argument" : "status",
        "state" : "retry",
        "expected" : "DELETING"
      }, {
        "matcher" : "path",
        "argument" : "status",
        "state" : "failure",
        "expected" : "DELETE_FAILED"
      } ]
    },
    "PolicyGenerationCompleted" : {
      "description" : "Wait until policy generation is completed",
      "delay" : 2,
      "maxAttempts" : 60,
      "operation" : "GetPolicyGeneration",
